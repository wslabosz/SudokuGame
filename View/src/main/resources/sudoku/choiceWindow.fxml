<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="Pane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sudoku.ChoiceWindowControl">
   <children>
      <Label fx:id="diffLabel" layoutX="190.0" layoutY="80.0" text="%ChooseDiff" textAlignment="CENTER">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="difficultyChoiceBox" layoutX="182.0" layoutY="128.0" prefHeight="26.0" prefWidth="236.0" />
      <Button fx:id="buttonStartGame" layoutX="280.0" layoutY="200.0" mnemonicParsing="false" onAction="#onActionButtonStartGame" text="%startButton" />
      <RadioButton fx:id="enLangButton" layoutX="20.0" layoutY="10.0" mnemonicParsing="false" onAction="#changeLangEn" text="%radioButtonEn">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="plLangButton" layoutX="20.0" layoutY="30.0" mnemonicParsing="false" onAction="#changeLangPl" text="%radioButtonPl">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </RadioButton>
      <Label fx:id="authorsLabel" layoutX="20.0" layoutY="369.0" />
   </children>
</AnchorPane>
