<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sudoku.ChoiceWindowControl">
   <children>
      <Label fx:id="diffLabel" layoutX="190.0" layoutY="80.0" textAlignment="CENTER">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="difficultyChoiceBox" layoutX="182.0" layoutY="128.0" prefHeight="26.0" prefWidth="236.0" />
      <Button fx:id="buttonStartGame" layoutX="280.0" layoutY="200.0" mnemonicParsing="false" onAction="#onActionButtonStartGame"/>
      <RadioButton fx:id="enLangButton" layoutX="20.0" layoutY="10.0" mnemonicParsing="false" onAction="#changeLangEn">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </RadioButton>
      <RadioButton fx:id="plLangButton" layoutX="20.0" layoutY="30.0" mnemonicParsing="false" onAction="#changeLangPl">
         <font>
            <Font name="Comic Sans MS" size="12.0" />
         </font>
      </RadioButton>
      <Label fx:id="authorsLabel" layoutX="20.0" layoutY="369.0" text="Label" />
   </children>
</AnchorPane>
